page {
  height: 100%;
  width: 100%;
  background: #2B70F0;
  overflow: hidden;
}
.container {
  background: #fff;
  height: 100%;
   overflow-y: auto;
   -webkit-overflow-scrolling: touch;
}
.bottom{
  padding-bottom: 80rpx;
  box-sizing: border-box;
}
.banner {
  height: 207rpx;
  background: #2B70F0;
  color: #fff;
  display: flex;
  align-items: center;
  padding: 0 20rpx; 
  justify-content: space-between;
}
.banner .competition_logo {
  width: 120rpx;
  height: 120rpx;
}
.competition_logo image{
  width: 120rpx;
  height: 120rpx;
}
.competition_info {
  width:560rpx;
  font-size: 28rpx;
}
.competition_title {
  height: 96rpx;
  line-height: 48rpx;
}
.competition_pos, .team_num {
  display: flex;
  align-items: center;
  height: 33rpx;
  font-size: 24rpx;
  margin-top: 10rpx;
  color: rgba(255, 255, 255, .77);
}
.team_num {
  margin-left: 20rpx;
}
.competition_detail {
  display: flex;
}
.competition_pos image, .team_num image{
  width: 32rpx;
  height: 36rpx;
  margin-right: 6rpx;
}
/*导航栏  */
.nav {
  width: 100%;
  height: 100rpx;
  display: flex;
  box-shadow: 0 2px 4px 0 #DFE3E2;
}
.nav view {
  width: 33.3%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.nav text {
  width: 160rpx;
  height: 60rpx;
  border-radius: 100rpx;
  background: #fff; 
  text-align: center;
  font-size: 28rpx;
  color: #666;
  line-height: 60rpx;
}
.nav text.tabSel{
  color: #fff;
  background: #2B70F0;
}
.break {
  height: 20rpx;
  background: #f7f7f7;
}
/*队员  */
.card_num {
  display: flex;
  background: #ebebeb;
  height: 80rpx;
  font-size: 28rpx;
  color: #838383;
}
.card_num view {
  width: 50%;
  height: 100%;
  line-height: 80rpx;
  text-align: center;
}
.card_num view text {
  display: inline-block;
  height:76rpx;
}
.card_num view:first-child {
  border-right: 1px solid #fff;
}
.card_num view .cardSel {
  color: #333;
  border-bottom: 2px solid #2B70F0;
}
.all_player {
  padding: 0 30rpx;
  box-sizing: border-box;
}
.allplsyer_item {
  height: 120rpx;
  padding: 20rpx 0;
  display: flex;
  border-bottom: 1px solid #ebebeb;
}
.allplsyer_item .header_wrap {
  width: 120rpx;
  height: 120rpx;
}
.allplsyer_item .header_wrap image{
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
}
.player_info {
  width: 550rpx;
  padding-left: 20rpx;
  font-size: 28rpx;
}
.player_name {
  height: 50rpx;
  display: flex;
  align-items: center;
}
.player_name .name_wrap {
  color: #444;
  padding: 0 0 0 20rpx;
  line-height: 35rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.player_name .leader_wrap, .player_name .team_wrap{
  padding: 0 20rpx;
  background: #acd2fe;
  color: #2B70F0;
  font-size: 26rpx;
  border-radius: 30rpx;
  margin-left: 20rpx;
  min-width: 70rpx;
  height: 35rpx;
  text-align: center;
  line-height: 35rpx;
  overflow: hidden;
}
.regisert_wrap {
  width: 125rpx;
  display: flex;
  align-items: center;
  padding: 0 20rpx;
  color: #acd2fe;
  font-size: 26rpx;
  border-radius: 30rpx;
  margin-left: 20rpx;
  border: 1px solid #acd2fe;  
}
.player_name image {
  width: 36rpx;
  height: 36rpx;
}
.player_pos {
  display: flex;
  height: 50rpx;
  align-items: center;
  margin-top: 20rpx;
}
.player_pos view {
  padding: 0 20rpx;
  font-size: 26rpx;
  background: #f6f9f9;
  color: #b0b0b0;
  /* border: 1px solid #2B70F0; */
  margin-right: 20rpx;
  border-radius: 30rpx;
}
.allplsyer_item:last-of-type {
  border-bottom: none;
}
.com_playerlist, .com_workerlist{
  padding: 0 20rpx;
}
.compete_item>view:first-child {
  height: 80rpx;
  color: #333;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 20rpx;
  border-bottom: 1px solid #ebebeb;
  font-size: 32rpx;
}
.compete_item>view:first-child image {
  width: 32rpx;
  height: 32rpx;
}
.com_player, .com_worker {
  display: flex;
  align-items: center;
  height: 120rpx;
  border-bottom: 1px solid #ebebeb;  
  padding: 20rpx 0;
}
.com_player .header_wrap, .com_worker .header_wrap{
  width: 120rxp;
  height: 120rpx;
}
.com_player .header_wrap image, .com_worker .header_wrap image{
  width: 120rpx;
  height: 120rpx;
}
.list_title {
  height: 80rpx;
  line-height: 80rpx;
  /* padding: 0 20rpx; */
  border-bottom: 1px solid #ebebeb;
}
.list_title text {
  padding: 0 20rpx;
  margin-left: 20rpx;
  border-left: 3px solid #2B70F0;
  font-size: 26rpx;
  color: #333;
}
/*射手榜  */
.shooter_table {
  padding: 0 30rpx;
}
.shooter_table_title {
  height: 78rpx;
  border-bottom: 1px solid #ebebeb;
  display: flex;
  align-items: center;
  font-size:24rpx;   
  justify-content: space-between;
  color: #b0b0b0;
}
.shooter_table_item{
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 100rpx;
  font-size: 26rpx;
  color: #1c1c1c;
  border-bottom: 1px solid #ebebeb;
}
.shooter_table_item:last-of-type {
  border-bottom: none;
}
.shooter_table_item>view:first-child {
  width:48rpx;
  text-align: center;
}
.shooter_table_item view:nth-child(2) {
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 570rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.shooter_table_item view:nth-child(2)>view {
  width: 26%;
  display: flex;
  align-content: centr;
  /* justify-content: center; */
}
.shooter_table_item view:nth-child(2) text {
  max-width: 510rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;  
}
.shooter_table_item image{
  width: 38rpx;
  height: 38rpx;
  margin-right: 20rpx;
  border-radius: 50%;
}
.shooter_table_item view:nth-child(3) {
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 72rpx;
}
/*对阵表  */
.year_title {
  display: flex;
  height: 80rpx;
  align-items: center;
  justify-content: space-between;
  padding: 0 20rpx;
  font-size: 28rpx;
  color: #333;
}
.year_item {
  margin-bottom: 20rpx;
}
.year_title image {
  width: 32rpx;
  height: 32rpx;
}
.yaerlist_item {
  margin: 0 auto;
  padding-bottom: 20rpx;
  border-bottom: 1px solid #ebebeb;
}
.yaer_list {
  /* box-shadow: 2px -1px 6px 2px #ccc; */
  width: 710rpx;
  margin: 0 auto;
}
.y_title {
  height: 80rpx;
  display: flex;
  align-items: center;
  padding: 0 20rpx;
  justify-content: space-between;
  font-size: 28rpx;
  color: #333;
}
.y_title view:first-child {
  display: flex;
  height: 100%;
  align-items: center;
}
.y_title image {
  width: 40rpx;
  height: 40rpx;
  margin-right: 10rpx;
}
.y_time {
  height: 45rpx;
  line-height: 45rpx;
  color: #b4b4b4;
  text-align: center;
  font-size: 26rpx;
  margin-bottom: 20rpx;
}
.y_tab {
  height: 50rpx;
  display: flex;
  justify-content: space-between;
  font-size: 26rpx;
  color: #333;
  line-height: 50rpx;
}
.y_tab .left,.y_tab .right{
  flex: 1;
  display: flex;
  overflow: hidden;
text-overflow:ellipsis;
white-space: nowrap;
}
.y_tab .fen{
  width: 80rpx;
  text-align: center;
}
.y_tab .left{
  justify-content: flex-end;

}

.y_tab .teamName{
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.y_tab image {
  height: 50rpx;
  width: 50rpx;
  margin: 0 20rpx;
  border-radius: 50%;
}
.y_tab view:nth-child(3) {
  margin: 0 20rpx;
}
.y_tab .right{
  justify-content: flex-start;
  margin:0;
}
.y_tab .right image{
  margin:0;
}
.btn_team {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: 100;
  display: flex;
  height: 80rpx;
  align-items: center;
  justify-content: center;
  background: #f7f7f7;
  border-top: 1px solid #ebebeb;
}
.btn_team button{
  border:none;
  padding: 0;
  color: #2B70F0;
}
.isNull {
  padding: 85px 0;
  display: flex;
  align-items: center;
  flex-direction: column;
  color: #999;
  font-size: 28rpx;
}
.isNull image {
  width: 216rpx;
  height: 101rpx;
  margin-bottom: 20rpx;
}

.edit{
  flex: 1;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}
.edit image{
  width: 30rpx;
  height: 30rpx;
  margin-right: 10rpx;
}
.edit text{
  width: 80rpx;
  text-align: center;
}